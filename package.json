{
    "private": false,
    "name": "react-lattice",
    "version": "1.0.3",
    "description": "A set of flexible React components that simplify responsive web development.",
    "keywords": [
        "react",
        "layout",
        "component",
        "responsive",
        "grid",
        "flexbox"
    ],
    "author": "Simeon de Dios <simeon@nascentdigital.com>",
    "license": "MIT",
    "repository": "nascentdigital/react-lattice",
    "main": "dist/index.js",
    "module": "dist/index.es.js",
    "jsnext:main": "dist/index.es.js",
    "engines": {
        "node": ">=8",
        "npm": ">=5"
    },
    "scripts": {
        "test": "cross-env CI=1 react-scripts-ts test --env=jsdom",
        "test:watch": "react-scripts-ts test --env=jsdom",
        "build": "rollup -c",
        "build:storybook": "build-storybook -s docs -o docs/storybook",
        "start": "rollup -c -w",
        "prepare": "npm run build",
        "deploy": "gh-pages -d docs",
        "storybook": "start-storybook -p 9001"
    },
    "dependencies": {},
    "peerDependencies": {
        "prop-types": "^15.5.4",
        "react": "^16.0.0",
        "react-dom": "^16.0.0",
        "react-jss": "^10.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.6.4",
        "@babel/preset-env": "^7.6.3",
        "@babel/preset-react": "^7.6.3",
        "@babel/preset-typescript": "^7.6.0",
        "@babel/runtime": "^7.6.3",
        "@storybook/addon-knobs": "^5.2.4",
        "@storybook/react": "^5.2.3",
        "@svgr/rollup": "^4.3.2",
        "@types/jest": "^24.0.18",
        "@types/react": "^16.9.2",
        "@types/react-dom": "^16.9.0",
        "@types/storybook__react": "^4.0.2",
        "babel-loader": "^8.0.6",
        "cross-env": "^6.0.0",
        "gh-pages": "^2.1.1",
        "mini-css-extract-plugin": "^0.8.0",
        "react": "^16.9.0",
        "react-docgen-typescript-loader": "^3.3.0",
        "react-dom": "^16.9.0",
        "react-jss": "^10.0.0",
        "react-scripts-ts": "^3.1.0",
        "rollup": "^1.21.4",
        "rollup-plugin-babel": "^4.3.3",
        "rollup-plugin-commonjs": "^10.1.0",
        "rollup-plugin-node-resolve": "^5.2.0",
        "rollup-plugin-peer-deps-external": "^2.2.0",
        "rollup-plugin-postcss": "^2.0.3",
        "rollup-plugin-typescript2": "^0.24.2",
        "rollup-plugin-url": "^2.2.2",
        "typescript": "^3.6.3"
    },
    "files": [
        "dist"
    ]
